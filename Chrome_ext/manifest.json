{
  "manifest_version": 2,

  "name": "Ann's geolocation",
  "description": "This extension shows city coordinates and post them to google spreadsheet",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Update location!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://localhost:8000/",
    "geolocation"
  ]
}
